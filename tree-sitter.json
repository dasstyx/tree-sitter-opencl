{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "opencl",
      "camelcase": "OpenCl",
      "title": "OpenCL",
      "scope": "source.opencl",
      "file-types": [
        "cl"
      ],
      "injection-regex": "^opencl$",
      "class-name": "TreeSitterOpencl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parse the OpenCL syntax source code",
    "authors": [
      {
        "name": "dasstyx",
        "email": "dasstyx@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-opencl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}